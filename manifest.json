{
  "manifest_version": 2,

  "name": "Replace URL",
  "description": "This extension replaces '.com', '.org' and '.ca' with '.http://ezproxy.library.ubc.ca'",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
